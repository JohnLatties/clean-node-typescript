{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests --runInBand --no-cache --detectOpenHandles",
    "test:watch": "npm test -- --watchAll --coverage",
    "test:staged": "npm test -- --findRelatedTests",
    "start": "node dist/index.js",
    "start:scraping": "node dist/main/scrapingServer.js",
    "start:api": "node dist/main/apiServer.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "dev:scraping": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/main/scrapingServer.ts",
    "dev:api": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/main/apiServer.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.3",
