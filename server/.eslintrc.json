{
  "env": {
    "es2020": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "rules": {
        "@typescript-eslint/no-unused-vars": [
          2,
          {
            "args": "none"
          }
        ],
        "eslintno-multiple-empty-lines": "off",
        "no-dupe-class-members": "off"
      }
    }
  ],
  "rules": {}
}